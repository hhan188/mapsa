SELECT *
FROM MAPSA.PERSON
ORDER BY AGE ASC;


select count(age), age
from person
group by age;


select avg(age)
from person;


select * from person where age <= 40 and age >= 30;


select id,name,age,gender from person where name like '%J%';


update mapsa.person set age=35 where name='John';


