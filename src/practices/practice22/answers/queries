create table if not exists person(
    id int auto_increment not null primary key ,
    name varchar(50),
    email varchar(50),
    phone_number varchar(50),
    age int,
    gender varchar(50),
    date_of_birth date,
    occupation varchar(50),
    education_level varchar(50),
    marital_status varchar(50),
    income decimal(10,2)


);



