select person.name , count(*) from person join post on person_id = person.id group by person.name;
-- 10
select person.name , count(*) from person join post on person_id = person.id group by person.name having count(*) > 2;
-- 11
select name , income from person order by income desc;
-- 12
select avg(income) from person;
select * from person where income > 74750;
select * from person where income > (select avg(income) from persons);
-- 13
select * from person where email like '%gmail.com';
-- 14
alter table person add nationality varchar(50);
select * from person;
select nationality , count(*) from person group by nationality;
-- 15
select * from person where name like '%smith%' and age > 40;
-- 16
select * from person where length(name) >= 5 and name like '%a';
-- 17
select name , age as "Years" from person;
-- 18
select name , round(age) from person;
-- 19
select max(age) from person;
update person set email = "oldest@example.com" where age = 46;
-- 20
delete p1 from person p1 join person p2 where p1.id != p2.id and p1.name = p2.name;
-- 21
select person.name , post.title , post.content from person join post on person_id = person.id;
-- 22
select person.name , count(*) from person join post on person_id = person.id group by person.name;
-- 23
select person.name , count(*) from person join post on person_id = person.id group by person.name having count(person_id) = 0;
-- 24
select avg(person.age) from person join post on person_id = person.id having count(person_id) >= 3;
-- 25
select person.name , post.content from person join post on person_id = person.id where post.content like "%SQL%";
-- 26
select max(length(post.content)) from post;
select person.name , max(length(post.content)) from person join post on person_id = person.id group by person.name having max(length(post.content)) = 61;
-- 27
select post.title , post.content , post.likes from post;
-- 28
select post.title , post.content , person.name , person.age from post join person on person_id = person.id order by person.age desc;
-- 29
select person.marital_status , count(*) from person join post on person_id = person.id group by person.marital_status having count(post.content) > 0;
-- 30
select * from person join post on person_id = person.id having person.age between 25 and 40;
-- 31
select * from person join post on person_id = person.id order by person.education_level asc;
select * from person join post on person_id = person.id order by person.income desc;
-- 32

-- 33
select avg() from person join post on person_id = person.id;
select person.name , count(person_id) from person join post on person_id = person.id group by person.name;
-- 34
select * from person join post on person_id = person.id where person.email like "%gmail.com";
-- 35
alter table post add column date date;
select person.name , post.date from person join post on person_id = person.id order by post.date desc;
-- 36
select person.name , post.date from person join post on person_id = person.id order by post.date desc;
select person.name , max(post.date) from person join post on person_id = person.id group by person.name order by max(post.date) desc;
-- 37
select * from post where post.likes > 10;
-- 38
select person.name , sum(likes) from person join post on person_id = person.id group by person.name;
-- 39
select person.name , post.content from person join post on person_id = person.id order by length(post.content) desc;
select * from post;
-- 40
select person.name , count(*) from post join person on person_id = person.id group by person.name order by count(*) desc limit 1;