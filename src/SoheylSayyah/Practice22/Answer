use mapsa;

create table person (
id int,
name varchar(50),
email varchar(200),
phone_number varchar(11),
age int,
gender varchar(10),
date_of_birth date,
occupation varchar(50),
education_level varchar(50),
marital_status varchar(20),
income int);
